    <validationRules>
        <fullName>Airport_staff_can_update_the_BIR_Tracer</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
    NOT(ISNEW()),
    NOT(ISCHANGED( LastModifiedDate )),
    Text( Airport__c ) != TEXT( $User.Airport__c )
)</errorConditionFormula>
        <errorMessage>You can update the BIR Tracer for your own airport only.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Comments_is_mandatory_for_Similar_Match</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL( Status__c, &apos;Similar Match&apos;),
ISBLANK(Comments__c)
)</errorConditionFormula>
        <errorDisplayField>Comments__c</errorDisplayField>
        <errorMessage>Please enter the baggage details in comments.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_cannot_be_Not_Responded</fullName>
        <active>true</active>
        <description>User cannot leave status as - Not Responded</description>
        <errorConditionFormula>AND(NOT(ISNEW()), ISPICKVAL(Status__c, &apos;Not Responded&apos;))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Please update the Status.</errorMessage>
    </validationRules>
