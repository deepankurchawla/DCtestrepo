    <validationRules>
        <fullName>Contents_is_mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
    OR(
        TEXT(BIR_Type__c) == &apos;AHL&apos;,
        TEXT(BIR_Type__c) == &apos;DPR&apos;
    ),
    ISBLANK(Contents__c)
)</errorConditionFormula>
        <errorDisplayField>Contents__c</errorDisplayField>
        <errorMessage>Contents is mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_Baggage_Color_is_mandatory</fullName>
        <active>true</active>
        <description>Other Baggage Color is mandatory if Baggage Color is Others</description>
        <errorConditionFormula>AND(
    TEXT( Baggage_Color__c ) == &apos;Other&apos;,
    ISBLANK( Other_Baggage_Color__c )
)</errorConditionFormula>
        <errorDisplayField>Other_Baggage_Color__c</errorDisplayField>
        <errorMessage>Other Baggage Color  is mandatory if Baggage Color is others</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_Baggage_Type_is_mandatory</fullName>
        <active>true</active>
        <description>Other Baggage Type is mandatory if Baggage type is Others</description>
        <errorConditionFormula>AND(
    TEXT( Baggage_Type__c ) == &apos;Other&apos;,
    ISBLANK( Other_Baggage_Type__c )
)</errorConditionFormula>
        <errorDisplayField>Other_Baggage_Type__c</errorDisplayField>
        <errorMessage>Other Baggage Type is mandatory if Baggage Type is others</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Total_Baggage_Weight_vs_Missing_Weight</fullName>
        <active>false</active>
        <description>Weight of Missing Baggage cannot be more than the Total Baggage Weight.</description>
        <errorConditionFormula>Case_No__r.Total_Baggage_Weight__c &lt; Case_No__r.Total_Weight_of_Missing_Baggage__c</errorConditionFormula>
        <errorMessage>Weight of all the Missing Baggage cannot be more than the Total Baggage Weight.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Total_Pieces_vs_Missing_Pieces</fullName>
        <active>false</active>
        <description>Pieces of Missing Baggage cannot be more than Total Pieces Checked In.</description>
        <errorConditionFormula>Case_No__r.Total_Pieces_Checked_In__c &lt;  Case_No__r.Number_of_Missing_Baggage__c</errorConditionFormula>
        <errorMessage>Pieces of all Missing Baggage cannot be more than the Total Pieces Checked In.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Weight_should_be_same</fullName>
